
<div class="row user">
	<div class="col-md-2">
		<div class="user profile">
			<%= render'newses/list', user: current_user%>
		</div>
	</div>
	<div class="col-md-10">
	    <div class="row">
			<div class="col-md-6">
				<div id="container">
					<div id="slide">
						<div class="new1"></div>
						<div class="new2"></div>
						<div class="new3"></div>
						<div class="new4"></div>
					</div>
				</div>
			</div>
	    </div>
	    <div class="row">
	    	<%= @newses.each do |n| %>
	    		<div class="col-md-4">
	    	    <div class="well user-news">
	    	    	<%= link_to newse_path %>
	    			<%= attachment_imge_tag n, :image, format: 'jpeg', fallback: "noimage.jpg", size: "100Ã—100" %>
	    			<%= news.title %>
	    			<%= news.text %>
	    		</div>
	    		</div>
	    	<% end %>
	    </div>
	</div>
		<% @top_headlines.each do |top_headline| %>
		<div class="col-md-4">
			<div class="well">

				<div class="img-box">
				<img src= <%= top_headline.urlToImage  %>>
				</div>
				<h3><%= top_headline.title  %></h3>

			</div>
	    </div>
		<% end %>
	</div>
</div>
