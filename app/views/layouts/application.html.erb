<!DOCTYPE html>
<html>
  <head>
    <title>LocateNews</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <header class="nav navbar-inverse new">
      <div class="container">
        <div class="logo">
          <h4><%= link_to 'LocateNews', newses_path, :style =>"color:white;" %></h4>
        </div>
        <div class="col-md-12">
        </div>
      </div>
    </header>
    <%= yield %>
  </body>
</html>
